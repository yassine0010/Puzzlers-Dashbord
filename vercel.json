{
  "buildCommand": "cd DashBordPuzzlers && npm run build",
  "outputDirectory": "DashBordPuzzlers/dist/DashBordPuzzlers/browser",
  "installCommand": "cd DashBordPuzzlers && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/DashBordPuzzlers/api/[...path]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
